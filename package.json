{
  "name": "nfl-pickem-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "sync:nfl": "node scripts/sync-nfl-data.js",
    "workers:dev": "wrangler dev",
    "workers:deploy": "wrangler publish --env production",
    "workers:build": "npx @cloudflare/next-on-pages",
    "db:export": "sqlite3 prisma/dev.db .dump > database-dump.sql",
    "db:d1-create": "wrangler d1 create nfl-pickem-db",
    "db:d1-migrate": "wrangler d1 execute nfl-pickem-db --file=./database-dump.sql --env production",
    "test:api": "node scripts/test-api-contracts.js",
    "test:api:production": "TEST_BASE_URL=https://your-production-url.com node scripts/test-api-contracts.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/adapter-d1": "^6.15.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "prisma": "^6.15.0",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vercel": "47.0.4",
    "wrangler": "^4.34.0"
  }
}
